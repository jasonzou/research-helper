{
  "name": "research-helper",
  "version": "3.0.0-beta",
  "description": "A paper / book / note management tool",
  "productName": "Research Helper",
  "author": "Hunt Feng <spacecurvature@gmail.com>",
  "private": false,
  "license": "AGPL-3.0",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "yarn run pouchdb-server --port 3000 -d remote_database &  quasar dev -m electron",
    "dev1": "pnpm run pouchdb-server --port 3000 -d remote_database &  quasar dev",
    "debug": "quasar build -m electron -T AppImage --debug",
    "build": "quasar build -m electron",
    "test:ci": "yarn run test:unit:ci --no-threads && yarn run test:component:ci",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "posttest:unit:ci": "rm -rf mydb*",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@citation-js/plugin-isbn": "^0.3.2",
    "@electron/remote": "^2.0.10",
    "@excalidraw/excalidraw": "^0.15.2",
    "@quasar/extras": "^1.16.4",
    "axios": "^0.21.4",
    "citation-js": "^0.6.8",
    "cytoscape": "^3.25.0",
    "cytoscape-cola": "^2.5.1",
    "electron-updater": "^5.3.0",
    "events": "^3.3.0",
    "golden-layout": "^2.6.0",
    "katex": "^0.16.7",
    "nodejs-file-downloader": "^4.12.1",
    "pdfjs-dist": "3.6.172",
    "pinia": "^2.1.3",
    "pouchdb": "^7.3.1",
    "pouchdb-find": "^7.3.1",
    "quasar": "^2.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vditor": "3.8.17",
    "veaury": "^2.3.12",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@quasar/app-vite": "^1.4.3",
    "@quasar/quasar-app-extension-testing": "^2.1.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.1.2",
    "@tauri-apps/cli": "^1.3.1",
    "@types/cytoscape": "^3.19.9",
    "@types/katex": "^0.16.0",
    "@types/node": "^12.20.55",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "1.3.2",
    "@vitejs/plugin-vue": "2.3.3",
    "@vitejs/plugin-vue-jsx": "~1.3.10",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.14.0",
    "electron": "^25.1.0",
    "electron-builder": "^23.6.0",
    "electron-packager": "^15.5.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.14.1",
    "pouchdb-server": "^4.2.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5",
    "vitest": "^0.15.2"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
